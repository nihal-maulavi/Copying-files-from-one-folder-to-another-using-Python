import shutil
from os.path import isfile, join
from os import listdir
import pandas as pd

folder = r"C:\Users\kbinm18453\Desktop\kreditbee\test\\"
pdf_files = [f for f in listdir(folder) 
             if isfile(join(folder, f))]
pdf_names = []
for pdf in pdf_files:
    a = pdf.split(".")
    pdf_names.append(a[0])
    
save_path = r"C:\Users\kbinm18453\Desktop\kreditbee\test2"

filename_sheet = pd.read_excel(r"C:\Users\kbinm18453\Desktop\kreditbee\test\file_names.xlsx")
for i in filename_sheet['filename']:
    for pdfs in pdf_names:
        if i == pdfs:
           shutil.copy(r""+folder + pdfs +".pdf", save_path)
